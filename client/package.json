{
  "name": "xiazhi-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-components": "^1.1.7",
    "@apollo/client": "^3.6.9",
    "@craco/craco": "^6.4.3",
    "@loadable/component": "^5.15.2",
    "ahooks": "^3.0.5",
    "antd": "^4.17.3",
    "axios": "^0.19.2",
    "dayjs": "^1.9.6",
    "graphql": "^16.1.0",
    "kiwi-intl": "^1.2.4",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "pinyin": "^3.0.0-alpha.5",
    "qs": "^6.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.0",
    "umi-request": "^1.4.0",
    "wbd-frontend-kit": "1.0.11",
    "web-vitals": "^0.2.4",
    "zustand": "^3.6.7"
  },
  "scripts": {
    "start": "cross-env REACT_APP_CONFIG_ENV=dev GENERATE_SOURCEMAP=false craco start",
    "build": "cross-env REACT_APP_CONFIG_ENV=prod craco build",
    "dev": "cross-env REACT_APP_CONFIG_ENV=dev craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint:ts": "tsc --noEmit && eslint src --ext .ts,.tsx ",
    "analyze": "source-map-explorer build/static/js/main.*",
    "script": "wbd-scripts",
    "gen": "fruits-chain-gc http://localhost:3000/graphql",
    "gen-w": "fruits-chain-gc http://localhost:3000/graphql -w",
    "test:config": "extintl config -o",
    "test:sync": "extintl sync",
    "test:start": "extintl start",
    "test:update": "extintl update"
  },
  "resolutions": {
    "@types/react": "^17",
    "prettier": "^2.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pretty-quick --staged",
      "npm run lint:ts"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@fruits-chain/eslint-config-preset": "^0.1.0",
    "@fruits-chain/graphql-codegen-preset": "^0.0.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^24.0.0",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/webpack-env": "^1.15.3",
    "add": "^2.0.6",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "craco-fast-refresh": "^1.1.0",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "pretty-quick": "^3.1.2",
    "webpack-bundle-analyzer": "^4.1.0",
    "yarn": "^1.22.19"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
