  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@prisma/client": "^3.15.2",
    "@types/micro-cors": "^0.1.2",
    "apollo-server-core": "^3.9.0",
    "apollo-server-koa": "^3.9.0",
    "bcrypt": "^5.0.1",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-jwt": "^4.0.3",
    "nexus": "^1.3.0",
    "nodemailer": "^6.7.5",
    "prisma": "^3.15.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/micro": "^7.3.7",
    "@types/nodemailer": "^6.4.4",
    "concurrently": "^7.2.2",
