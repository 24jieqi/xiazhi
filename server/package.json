{
  "name": "server",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --rs ./src/index.ts",
    "dev:typecheck": "tsc --noEmit --watch",
    "clean": "rm -rf dist",
    "prisma:migrate": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema=./src/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema ./src/prisma/schema.prisma",
    "nexus:generate": "ts-node --transpile-only src/graphql/schema",
    "generate": "yarn prisma:generate && yarn nexus:generate",
    "build": "yarn generate && tsc",
    "build:auto": "node scripts/build.js"
  },
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@prisma/client": "^3.15.2",
    "@types/micro-cors": "^0.1.2",
    "apollo-server-core": "^3.9.0",
    "apollo-server-koa": "^3.9.0",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.5",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-jwt": "^4.0.3",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "nexus": "^1.3.0",
    "node-fetch": "2.6.7",
    "node-xlsx": "^0.21.0",
    "nodemailer": "^6.7.5",
    "prisma": "^3.15.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa-static": "^4.0.2",
    "@types/micro": "^7.3.7",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.4",
    "concurrently": "^7.2.2",
    "esm": "^3.2.25",
    "micro-dev": "^3.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
}
